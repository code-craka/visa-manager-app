{
  "name": "visa_manager_frontend",
  "version": "0.3.2",
  "private": true,
  "scripts": {
    "android": "react-native run-android",
    "ios": "react-native run-ios",
    "lint": "eslint .",
    "start": "react-native start",
    "test": "jest",
    "test:e2e": "jest --config=jest.e2e.config.js",
    "test:coverage": "jest --coverage",
    "web": "webpack serve --config webpack.config.js --mode development --env-file .env.web",
    "build:web": "NODE_ENV=production webpack --config webpack.config.js --mode production --env-file .env.web",
    "build:web:analyze": "NODE_ENV=production ANALYZE=true webpack --config webpack.config.js --mode production --env-file .env.web",
    "start:web": "webpack serve --config webpack.config.js --mode development --open --env-file .env.web",
    "dev:web": "webpack serve --config webpack.config.js --mode development --hot --env-file .env.web",
    "preview:web": "NODE_ENV=production webpack serve --config webpack.config.js --mode production --env-file .env.web",
    "android:env": "PLATFORM=android react-native run-android",
    "build:android": "PLATFORM=android cd android && ./gradlew assembleDebug && cd ..",
    "build:production": "NODE_ENV=production webpack --config webpack.config.js --mode production --env-file .env.production",
    "android:clean": "cd android && ./gradlew clean && cd ..",
    "android:build": "cd android && ./gradlew assembleDebug && cd ..",
    "android:release": "cd android && ./gradlew assembleRelease && cd ..",
    "ios:clean": "cd ios && rm -rf build && xcodebuild clean && cd ..",
    "clean": "yarn android:clean && yarn ios:clean && rm -rf node_modules && yarn install",
    "reset": "yarn clean && npx react-native start --reset-cache",
    "setup:jdk": "chmod +x ./setup-jdk.sh && ./setup-jdk.sh",
    "doctor": "npx react-native doctor",
    "info": "npx react-native info"
  },
  "dependencies": {
    "@clerk/clerk-expo": "^2.14.16",
    "@react-native-async-storage/async-storage": "^2.2.0",
    "@react-native-community/netinfo": "^11.4.1",
    "@react-native/new-app-screen": "^0.80.2",
    "@react-navigation/bottom-tabs": "^7.4.5",
    "@react-navigation/native": "^7.1.17",
    "@react-navigation/stack": "^7.4.5",
    "@stackframe/react": "^2.8.27",
    "@testing-library/jest-native": "^5.4.3",
    "@testing-library/react-native": "^13.2.2",
    "@types/react-router-dom": "^5.3.3",
    "expo-auth-session": "^6.2.1",
    "expo-crypto": "^14.1.5",
    "expo-linking": "^7.1.7",
    "expo-web-browser": "^14.2.0",
    "react": "18.3.1",
    "react-dom": "^19.1.1",
    "react-native": "0.75.4",
    "react-native-elements": "^3.4.3",
    "react-native-gesture-handler": "^2.20.2",
    "react-native-paper": "^5.14.5",
    "react-native-reanimated": "^3.16.1",
    "react-native-safe-area-context": "^4.12.0",
    "react-native-screens": "^3.34.0",
    "react-native-vector-icons": "^10.3.0",
    "react-native-web": "^0.21.0",
    "react-router-dom": "^7.8.0"
  },
  "devDependencies": {
    "@babel/core": "^7.25.2",
    "@babel/preset-env": "^7.25.3",
    "@babel/preset-react": "^7.27.1",
    "@babel/runtime": "^7.25.0",
    "@react-native-community/cli": "15.1.2",
    "@react-native-community/cli-platform-android": "15.1.2",
    "@react-native-community/cli-platform-ios": "15.1.2",
    "@react-native/babel-preset": "0.76.1",
    "@react-native/eslint-config": "0.76.1",
    "@react-native/gradle-plugin": "0.76.1",
    "@react-native/metro-config": "0.76.1",
    "@react-native/typescript-config": "0.76.1",
    "@testing-library/jest-native": "^5.4.3",
    "@testing-library/react-native": "^13.2.2",
    "@types/jest": "^30.0.0",
    "@types/react": "^18.3.12",
    "@types/react-test-renderer": "^18.3.0",
    "babel-loader": "^10.0.0",
    "concurrently": "^9.2.0",
    "dotenv": "^17.2.1",
    "eslint": "^9.33.0",
    "glob": "^11.0.3",
    "html-webpack-plugin": "^5.6.3",
    "jest": "^30.0.5",
    "prettier": "3.6.2",
    "react-test-renderer": "18.3.1",
    "rimraf": "^6.0.1",
    "typescript": "5.9.2",
    "webpack": "^5.101.0",
    "webpack-bundle-analyzer": "^4.10.2",
    "webpack-cli": "^6.0.1",
    "webpack-dev-server": "^5.2.2"
  },
  "engines": {
    "node": ">=18"
  },
  "resolutions": {
    "cookie": "^0.7.0"
  }
}
