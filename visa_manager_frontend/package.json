{
  "name": "visa_manager_frontend",
  "version": "0.3.0",
  "private": true,
  "scripts": {
    "android": "react-native run-android",
    "ios": "react-native run-ios",
    "lint": "eslint .",
    "start": "react-native start",
    "test": "jest",
    "android:clean": "cd android && ./gradlew clean && cd ..",
    "android:build": "cd android && ./gradlew assembleDebug && cd ..",
    "android:release": "cd android && ./gradlew assembleRelease && cd ..",
    "ios:clean": "cd ios && rm -rf build && xcodebuild clean && cd ..",
    "clean": "yarn android:clean && yarn ios:clean && rm -rf node_modules && yarn install",
    "reset": "yarn clean && npx react-native start --reset-cache",
    "setup:jdk": "chmod +x ./setup-jdk.sh && ./setup-jdk.sh",
    "doctor": "npx react-native doctor",
    "info": "npx react-native info"
  },
